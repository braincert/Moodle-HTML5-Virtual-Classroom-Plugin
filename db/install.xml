<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="mod/braincert/db" VERSION="20120122" COMMENT="XMLDB file for Moodle mod/braincert"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="braincert" COMMENT="Default comment for braincert, please edit me" NEXT="braincert_manage_template">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true" NEXT="course"/>
        <FIELD NAME="course" TYPE="int" LENGTH="10" NOTNULL="true" DEFAULT="0" SEQUENCE="false" PREVIOUS="id" NEXT="name"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" PREVIOUS="course" NEXT="class_id"/>
        <FIELD NAME="class_id" TYPE="int" LENGTH="20" NOTNULL="true" SEQUENCE="false" PREVIOUS="name" NEXT="intro"/>
        <FIELD NAME="intro" TYPE="text" LENGTH="big" NOTNULL="false" SEQUENCE="false" PREVIOUS="class_id" NEXT="introformat"/>
